{
  "name": "arcm-pat",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --clear",
    "prebuild": "expo prebuild --npm",
    "prebuild:clean": "expo prebuild --clean --npm",
    "prebuild:production": "APP_VARIANT=production expo prebuild --clean --npm",
    "prebuild:pre-production": "APP_VARIANT=pre-production expo prebuild --clean --npm",
    "prebuild:staging": "APP_VARIANT=staging expo prebuild --clean --npm",
    "android": "expo run:android",
    "android:device": "expo run:android --device",
    "android:production": "APP_VARIANT=production expo run:android --variant release",
    "android:pre-production": "APP_VARIANT=pre-production expo run:android --variant release",
    "android:staging": "APP_VARIANT=staging expo run:android --variant release",
    "ios": "expo run:ios",
    "ios:device": "expo run:ios --device",
    "ios:production": "APP_VARIANT=production expo run:ios --configuration Release",
    "ios:pre-production": "APP_VARIANT=pre-production expo run:ios --configuration Release",
    "ios:staging": "APP_VARIANT=staging expo run:ios --configuration Release",
    "web": "expo start --web"
  },
  "dependencies": {
    "@react-navigation/drawer": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "dotenv": "16.4.5",
    "expo": "54.0.0",
    "expo-av": "~16.0.8",
    "expo-document-picker": "~14.0.8",
    "expo-font": "~14.0.10",
    "expo-image-picker": "~17.0.10",
    "expo-local-authentication": "~17.0.8",
    "expo-notifications": "~0.32.15",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-nfc-manager": "3.16.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "typescript": "5.9.2"
  },
  "private": true
}
